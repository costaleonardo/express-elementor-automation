# ContentFlow MVP - Environment Variables Template
# Copy this file to .env and fill in your actual values

# Asana Integration
ASANA_ACCESS_TOKEN=your_asana_personal_access_token_here
ASANA_WEBHOOK_SECRET=your_asana_webhook_secret_here

# WordPress Integration
WORDPRESS_API_URL=https://your-wordpress-site.com/wp-json/wp/v2
WORDPRESS_USERNAME=your_wordpress_username
WORDPRESS_APP_PASSWORD=your_wordpress_application_password

# Server Configuration
PORT=3000
NODE_ENV=development